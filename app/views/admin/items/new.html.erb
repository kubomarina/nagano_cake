<h3>商品新規登録</h3>
<%= form_with model: [:admin,@item], local:true do |f| %>
 <h6>商品画像<h6>
 <%= f.attachment_field :image %>
 <h6>商品名</h6>
 <%= f.text_field :name %>
 <h6>商品説明</h6>
 <%= f.text_area :introduction %>
 <h6>ジャンル</h6>
 <%= f.select :jenre, ['ケーキ', '焼き菓子', 'プリン', 'キャンディ']%>
 <h6>税抜き価格</h6>
 <%= f.text_field :price %><a>円</a>
 <h6>販売ステータス</h6>
 
 
 <%= f.submit '新規登録' %>
 <% end %>
